pgcd {
	<int, int> c = <105,42>;
	const int test = 0;
	int a = fst c;
	int b = snd c;
	while (a*b != test) {
		if (a > b) {
			int na = a - b;
			a = na;
		} else {
			int nb = b - a;
			b = nb;
		}
	}
	int res = a;
	if (res == test) {
		res = b;
	}
	print "pgcd(105,45)=";
	print res;
	print " ";
	print (res == 21);
}